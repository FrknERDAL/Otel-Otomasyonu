@using MvcOTEL.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "MüsteriGeçmiş";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>İşlemler</h2>
<br />
<table class="table table-danger">
    <tr>
        <th>Oda No</th>      
        <th>Alış Tarihi</th>
        <th>Veriş Tarihi</th>
    </tr>
    @foreach(var x in Model)
            {
                <tr>
                    <td>@x.TBLOTEL.NO</td>
                    <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy")</td>
                    <td>@Convert.ToDateTime(x.VERISTARIHI).ToString("dd/MM/yyyy")</td>                </tr>
            }
</table>

